{
  "id": "https://xviz.org/schema/core/timeseries_state.schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "XVIZ Timeseries State",
  "type": "object",
  "properties": {
    "timestamp": {
      "type": "number",
      "minimum": 0
    },
    "values": {
      "type": "array",
      "items": {
        "type": "array",
        "items": [
          {
            "type": "string"
          },
          {
            "$ref": "https://xviz.org/schema/core/value.schema.json"
          }
        ],
        "minItems": 2,
        "maxItems": 2,
        "additionalItems": false
      },
      "additionalItems": false
    },
    "object_id": {
      "type": "string"
    }
  },
  "required": [ "timestamp", "values" ],
  "additionalProperties": false
}
