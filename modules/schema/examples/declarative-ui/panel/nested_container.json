        },
        {
          "type": "metric",
          "streams": ["/vehicle/velocity"],
          "title": "Velocity"
        },
        {
          "type": "metric",
          "streams": ["/vehicle/acceleration"],
          "title": "Acceleration"
        }
      ]
    }
  ]
